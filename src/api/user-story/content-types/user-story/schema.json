{
  "kind": "collectionType",
  "collectionName": "user_stories",
  "info": {
    "singularName": "user-story",
    "pluralName": "user-stories",
    "displayName": "User Stories",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "media_type": {
      "type": "enumeration",
      "enum": [
        "image",
        "video"
      ]
    },
    "media_url": {
      "type": "string",
      "required": true
    },
    "music_url": {
      "type": "string"
    },
    "is_highlighted": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "is_flagged": {
      "type": "boolean",
      "default": false
    },
    "flag_reason": {
      "type": "string"
    },
    "is_archived": {
      "type": "boolean",
      "default": false
    },
    "is_deleted": {
      "type": "boolean",
      "default": false
    },
    "highlights_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-highlight.user-highlight",
      "mappedBy": "user_story"
    },
    "creator_id": {
      "type": "string"
    },
    "prayer_count": {
      "type": "integer",
      "default": 0
    },
    "user_prayers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-prayer.user-prayer",
      "mappedBy": "user_story"
    },
    "story_views": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::story-view.story-view",
      "mappedBy": "story_id"
    }
  }
}
