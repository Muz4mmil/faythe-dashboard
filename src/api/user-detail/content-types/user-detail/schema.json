{
  "kind": "collectionType",
  "collectionName": "user_details",
  "info": {
    "singularName": "user-detail",
    "pluralName": "user-details",
    "displayName": "User Details"
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true
  },
  "attributes": {
    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "profile_image": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Trans",
        "Not specified"
      ]
    },
    "dob": {
      "type": "date"
    },
    "country": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "address_line_1": {
      "type": "string"
    },
    "address_line_2": {
      "type": "string"
    },
    "pincode": {
      "type": "string"
    }
  }
}
