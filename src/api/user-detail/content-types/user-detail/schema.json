{
  "kind": "collectionType",
  "collectionName": "user_details",
  "info": {
    "singularName": "user-detail",
    "pluralName": "user-details",
    "displayName": "User Details",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_details"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "profile_image": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "MALE",
        "FEMALE"
      ]
    },
    "dob": {
      "type": "date"
    },
    "country": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "address_line_1": {
      "type": "string"
    },
    "address_line_2": {
      "type": "string"
    },
    "pincode": {
      "type": "string"
    },
    "relationship_status": {
      "type": "enumeration",
      "enum": [
        "SINGLE",
        "MARRIED"
      ]
    },
    "sobriety_date": {
      "type": "date"
    }
  }
}
