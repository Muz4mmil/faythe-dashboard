{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "user_chats": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-chat.user-chat",
      "mappedBy": "user_2"
    },
    "user_messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-message.user-message",
      "mappedBy": "senderId"
    },
    "phone": {
      "type": "string",
      "unique": true
    },
    "is_flagged": {
      "type": "boolean",
      "default": false
    },
    "flag_reason": {
      "type": "enumeration",
      "enum": [
        "Hate Speech",
        "Scam or Fraud",
        "False Information"
      ]
    },
    "is_deleted": {
      "type": "boolean",
      "default": false
    },
    "user_followers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-follower.user-follower",
      "mappedBy": "following_id"
    },
    "user_details": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-detail.user-detail",
      "mappedBy": "user_id"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "creator"
    },
    "post_follows": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post-follow.post-follow",
      "mappedBy": "user_id"
    },
    "blocked_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blocked-user.blocked-user",
      "mappedBy": "blocked_user_id"
    },
    "user_stories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-story.user-story",
      "mappedBy": "creator"
    },
    "user_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-comment.user-comment",
      "mappedBy": "creator"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "creator"
    },
    "event_interests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-interest.event-interest",
      "mappedBy": "user_id"
    },
    "user_highlights": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-highlight.user-highlight",
      "mappedBy": "user"
    }
  }
}
